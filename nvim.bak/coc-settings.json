{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "trace.server": "verbose",
      "filetypes": [
        "go"
      ]
    }
  },
  "tabnine.disable_filetypes": [
    "markdown",
    "asciidoc"
  ],
  "coc.source.emoji.filetypes": [
    "markdown",
    "asciidoc",
    "html"
  ],
  "coc.source.emoji.triggerCharacters": [
    ";"
  ],
  "rust-analyzer.check.command": "clippy",
  "workspace.ignoredFolders": [
    "$HOME",
    "$HOME/.cargo/**",
    "$HOME/.rustup/**"
  ],
  "ltex.enabled": [
    "markdown"
  ],
  "ltex.language": "fr",
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/19.1.2/clangd_19.1.2/bin/clangd",
  "snippets.ultisnips.pythonPrompt": false
}
